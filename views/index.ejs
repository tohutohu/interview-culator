<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul class="feed-list">
      <% items.forEach(function(feed){ %>
        <li class="feed">
          <a class="feed-title" href="<% feed.url %>"><% feed.title %></a>
          <p class="feed_date"><% feed.pubdate %></p>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
