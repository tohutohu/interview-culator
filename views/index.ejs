<!DOCTYPE html>

<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <%- include("./head.ejs") %>
  </head>
  <body>
    <%- include("./header") %>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul class="feed-list">
      <% items.forEach(function(item){ %>
        <li class="feed-item">
           <div class ="feed">
             <a class="feed-title" href="<%= item.url %>" target="_blank"><%= item.title %></a>
             <p class="feed_date"><%= item.pubdate %></p>
           </div>

        </li>
      <% }); %>
    </ul>
  </body>
</html>
